@bot.tree.command(name="dice", description="ã‚µã‚¤ã‚³ãƒ­ã‚’æŒ¯ã‚Šã¾ã™")
@app_commands.describe(sides="dice_notations")
async def dice(interaction: discord.Interaction, sides: int = 6):
    if sides < 2:
        await interaction.response.send_message("âš ï¸ You have to enter number more than 2", ephemeral=True)
        return

    result = random.randint(1, sides)
    await interaction.response.send_message(f"ğŸ² **{sides}dice_notations result:{result}**")